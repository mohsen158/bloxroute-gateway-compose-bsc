# bloXroute Gateway Configuration
# Copy this file to .env and fill in your values

# Log and SSL certificate paths
LOG_FILE_PATH=./logs
SSL_CERT_PATH=./ssl

# Blockchain network (default: BSC-Mainnet)
BLOCKCHAIN_NETWORK=BSC-Mainnet

# Blockchain node configuration
BLOCKCHAIN_NODE_PUBLIC_KEY=your_blockchain_node_public_key_here
NODE_IP=your_node_ip_address
NODE_PORT=30303

# Ethereum Node Record (ENR)
ENR=your_enr_string_here

# Your private key (keep this secure!)
PRIVATE_KEY=your_private_key_here

# Ethereum WebSocket URI
ETH_WS_URI=ws://your_eth_node_ip:8546
